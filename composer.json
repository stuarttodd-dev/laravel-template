{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^2.0",
        "laravel/framework": "^12.0",
        "laravel/jetstream": "^5.2",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.8",
        "sentry/sentry-laravel": "^4.3",
        "tightenco/ziggy": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.12",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.14",
        "laravel/sail": "^1.18",
        "laravel/telescope": "^5.0",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "phpmd/phpmd": "^2.15",
        "phpstan/phpstan": "^1.12",
        "rector/rector": "^1.2",
        "spatie/laravel-ignition": "^2.9",
        "squizlabs/php_codesniffer": "^3.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": "npm run build && php -r \"is_dir('database') ?: mkdir('database', 0777, true); if (file_exists('database/database.sqlite')) unlink('database/database.sqlite'); touch('database/database.sqlite');\" && ./vendor/bin/pest",
        "phpcs": "./vendor/bin/phpcs app tests",
        "phpcbf": "./vendor/bin/phpcbf app tests",
        "phpmd": "./vendor/bin/phpmd app/ text phpmd.xml",
        "phpstan": "./vendor/bin/phpstan analyse --memory-limit=1G",
        "phpstan:baseline": "./vendor/bin/phpstan analyse --memory-limit=1G --generate-baseline",
        "rector:dry-run": "./vendor/bin/rector process --dry-run",
        "rector": "./vendor/bin/rector process",
        "test:coverage": "php -r \"is_dir('database') ?: mkdir('database', 0777, true); if (file_exists('database/database.sqlite')) unlink('database/database.sqlite'); touch('database/database.sqlite');\" && XDEBUG_MODE=coverage ./vendor/bin/pest --coverage --min=100",
        "coverage": [
            "@test:coverage"
        ],
        "standards:check": [
            "@phpcs",
            "@phpmd",
            "@phpstan",
            "@rector:dry-run"
        ],
        "standards:fix": [
            "@phpcbf",
            "@rector"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    }
}
